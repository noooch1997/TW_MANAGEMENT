        "from datetime import datetime, timedelta\n",
        "from typing import List, Dict\n",
        "import pandas as pd\n",
        "\n",
        "# Configuraci√≥n del sistema\n",
        "DIAS_GUARDIA = 10\n",
        "DIAS_DESCANSO = 10\n",
        "\n",
        "# S√≠mbolos\n",
        "SIMBOLO_GUARDIA = \"üî¥\"\n",
        "SIMBOLO_DESCANSO = \"üîµ\"\n",
        "\n",
        "class Trabajador:\n",
        "    def __init__(self, nombre: str, guardia: str, fecha_inicio: datetime):\n",
        "        self.nombre = nombre\n",
        "        self.guardia = guardia\n",
        "        self.fecha_inicio = fecha_inicio\n",
        "        self.estado = \"GUARDIA\"\n",
        "        self.rotacion = \"\"\n",
        "        self.actualizar_estado(datetime(2025, 3, 1))  # Fecha base 01/03/2025\n",
        "\n",
        "    def actualizar_estado(self, fecha_actual: datetime):\n",
        "        delta = (fecha_actual - self.fecha_inicio).days\n",
        "        ciclo_total = DIAS_GUARDIA + DIAS_DESCANSO\n",
        "\n",
        "        # L√≥gica mejorada para rotaciones\n",
        "        if delta < 0:\n",
        "            dias_faltantes = abs(delta)\n",
        "            self.estado = \"DESCANSO\"\n",
        "            self.rotacion = f\"Sube HOY\" if dias_faltantes == 1 else f\"Sube en {dias_faltantes} d√≠as\"\n",
        "        else:\n",
        "            ciclo = delta // ciclo_total\n",
        "            dia_ciclo = delta % ciclo_total\n",
        "\n",
        "            if dia_ciclo < DIAS_GUARDIA:\n",
        "                self.estado = \"GUARDIA\"\n",
        "                dias_restantes = DIAS_GUARDIA - dia_ciclo\n",
        "                self.rotacion = f\"Baja HOY\" if dias_restantes == 1 else f\"Baja en {dias_restantes} d√≠as\"\n",
        "            else:\n",
        "                self.estado = \"DESCANSO\"\n",
        "                dias_restantes = ciclo_total - dia_ciclo\n",
        "                self.rotacion = f\"Sube HOY\" if dias_restantes == 1 else f\"Sube en {dias_restantes} d√≠as\"\n",
        "\n",
        "class SistemaGuardias:\n",
        "    def __init__(self, trabajadores: List[Dict]):\n",
        "        self.trabajadores = [\n",
        "            Trabajador(trab['nombre'], trab['guardia'], trab['fecha_inicio'])\n",
        "            for trab in trabajadores\n",
        "        ]\n",
        "\n",
        "    def generar_reporte(self, fecha_actual: datetime) -> pd.DataFrame:\n",
        "        return pd.DataFrame([{\n",
        "            'Nombre': t.nombre,\n",
        "            'Guardia': t.guardia,\n",
        "            'Estado': t.estado,\n",
        "            'S√≠mbolo': SIMBOLO_GUARDIA if t.estado == \"GUARDIA\" else SIMBOLO_DESCANSO,\n",
        "            'Rotaci√≥n': t.rotacion\n",
        "        } for t in self.trabajadores])\n",
        "\n",
        "# Configuraci√≥n final con cronograma exacto\n",
        "TRABAJADORES = [\n",
        "    {'nombre': 'Mar√≠a', 'guardia': 'A', 'fecha_inicio': datetime(2025, 2, 20)},  # Baja el 01/03\n",
        "    {'nombre': 'Kim', 'guardia': 'B', 'fecha_inicio': datetime(2025, 2, 25)},    # Baja el 06/03\n",
        "    {'nombre': 'Sherly', 'guardia': 'C', 'fecha_inicio': datetime(2025, 3, 2)},  # Sube el 02/03\n",
        "    {'nombre': 'Jorge', 'guardia': 'D', 'fecha_inicio': datetime(2025, 3, 7)},\n",
        "    {'nombre': 'Sebastian', 'guardia': 'E', 'fecha_inicio': datetime(2025, 3, 7)}\n",
        "]\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    sistema = SistemaGuardias(TRABAJADORES)\n",
        "\n",
        "    # Reporte al 01/03/2025\n",
        "    print(\"\\n‚ïê\"*30 + \" REPORTE 01/03/2025 \" + \"‚ïê\"*30)\n",
        "    print(sistema.generar_reporte(datetime(2025, 3, 1)).to_string(index=False))\n",
        ""
